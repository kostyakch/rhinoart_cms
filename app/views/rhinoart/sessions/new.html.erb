<% provide(:title, t(:_AUTORIZATION)) %>








<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>
      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <br><%= f.submit t("Sign in"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>